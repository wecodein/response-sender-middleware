{
    "name": "wecodein/response-sender-middleware",
    "description": "PSR-15 compliant implementation of middleware for converting PSR-7 Response to HTTP output.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "wecodein",
        "response-sender-middleware"
    ],
    "authors": [
        {
            "name": "Dusan Vejin",
            "email": "dutekvejin@gmail.com",
            "homepage": "https://github.com/dutekvejin"
        }
    ],
    "require": {
        "php": "^7.0",
        "http-interop/http-middleware": "^0.4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2",
        "friendsofphp/php-cs-fixer": "^2.0",
        "php-mock/php-mock-phpunit": "^2.0",
        "http-interop/http-factory-guzzle": "^0.3.1"
    },
    "autoload": {
        "psr-4": {
            "WeCodeIn\\Http\\ServerMiddleware\\Middleware\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WeCodeIn\\Http\\ServerMiddleware\\Middleware\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always",
        "cs-fix": "php-cs-fixer fix --config=.php_cs",
        "cs-check": "php-cs-fixer fix --config=.php_cs -v --diff --dry-run"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
